let hehe = "hehe";

struct Vehicle {
    fn run() {
        print("Vehicle is running", hehe);
    }
}

struct Car {
    fn run(){
        let vehicle = Vehicle();
        vehicle.run();
        print("Car running lol", hehe);
        return vehicle;
    }
}

let car = Car();
let v = car.run();
print("BEFORE", v);
print("BEFORE",car.run().sad);
car.run().sad = 999;
print("AFTER", car.run().sad);