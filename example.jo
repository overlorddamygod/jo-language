struct Vehicle {
    fn run() {
        self.engine = true;
        print("Vehicle is running", self.engine);
    }
}

struct Car {
    fn Create(name) {
        self.name = name;
        print("Car", self.name, "created");
        return self;
    }
}
let self = Car().Create("Ferrari");

let car = Car().Create("BMW");
print(car.name);